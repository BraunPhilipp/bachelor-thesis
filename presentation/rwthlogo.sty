	% y-range RWTHAACHEN-C  1.206-2.335  1.228-2.319 RWTH  1.227-2.314 AACHEN
	\pgfpathmoveto{\pgfqpoint{#1}{#2}}
	\pgfpathlineto{\pgfqpoint{#3}{#2}}
	\pgfpathlineto{\pgfqpoint{#3}{#4}}
	\pgfpathlineto{\pgfqpoint{#1}{#4}}
	\pgfpathclose
	\pgfusepath{clip}
	}

